{
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "http://localhost:3001",
    "distDir": "../dist"
  },
  "package": {
    "productName": "alere",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "fs": {
         "createDir": true,
         "readDir": true,
         "readFile": true,
         "writeFile": true,
         "scope": ["$APPCONFIG", "$APPCONFIG/*"]
      },
      "dialog": {
        "open": true,
        "save": true
      },
      "path": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "Finance",
      "copyright": "E. Briot",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.alere",
      "longDescription": "Personal Finance Management",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "Personal Finance Management",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 1024,
        "resizable": true,
        "maximized": false,
        "title": "Alere",
        "center": false,
        "width": 1700
      }
    ]
  }
}
