    "typescript": "~4.1.3",
    "vite": "^2.3.2"
  },
  "scripts": {
    "start": "concurrently -n 'vite,tsc,eslint' -c 'white,green,blue' 'yarn run nochecks --clearScreen false' 'tsc -w --preserveWatchOutput' 'yarn run lint:watch'",
    "nochecks": "VITE_API_URL=127.0.0.1:8000 vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint:watch": "esw -w --changed --ext .tsx",
    "lint": "eslint --ext .ts,.tsx --ignore-path .gitignore .",
    "test": "jest --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
