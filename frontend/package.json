    "@typescript-eslint/parser": "^4.24.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "babel-eslint": "^10.1.0",
    "concurrently": "^6.1.0",
    "d3-scale-chromatic": "^2.0.0",
    "eslint": "^7.26.0",
